<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.spring_mp.quartz.mapper.TJobMapper">

        <!-- 通用查询映射结果 -->
        <resultMap id="BaseResultMap" type="com.example.spring_mp.quartz.entity.TJob">
                <result column="id" property="id"/>
                    <result column="job_name" property="jobName"/>
                    <result column="job_group" property="jobGroup"/>
                    <result column="cron" property="cron"/>
                    <result column="last_exec_time" property="lastExecTime"/>
                    <result column="remark" property="remark"/>
                    <result column="status" property="status"/>
        </resultMap>

        <!-- 通用查询结果列 -->
        <sql id="baseJoinMap">
                ,${prefix}.id as ${prefix}_id
                ,${prefix}.job_name as ${prefix}_job_name
                ,${prefix}.job_group as ${prefix}_job_group
                ,${prefix}.cron as ${prefix}_cron
                ,${prefix}.last_exec_time as ${prefix}_last_exec_time
                ,${prefix}.remark as ${prefix}_remark
                ,${prefix}.status as ${prefix}_status
        </sql>

</mapper>
